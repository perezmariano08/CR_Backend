{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://api-coparelampago.vercel.app/$1"
      }
    ]
}